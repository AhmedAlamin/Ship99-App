<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_request">

    <fragment
        android:id="@+id/navigation_request"
        android:name="com.Ship99_Official.ship99.Ui.RequestFragment"
        android:label="@string/Request"
        tools:layout="@layout/fragment_request" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.Ship99_Official.ship99.Ui.HistoryFragment"
        android:label="@string/History"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_track"
        android:name="com.Ship99_Official.ship99.Ui.TrackFragment"
        android:label="@string/Track"
        tools:layout="@layout/fragment_track" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.Ship99_Official.ship99.Ui.ProfileFragment"
        android:label="@string/Profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.Ship99_Official.ship99.Ui.ProfileFragment"
        android:label="@string/Profile"
        tools:layout="@layout/settings" />

</navigation>